@model IEnumerable<ERP_GMEDINA.Models.tbHistorialCargos>

@{
    ViewBag.Title = "Historial Cargos";
    Layout = "~/Views/Shared/_LayoutBase.cshtml";
}


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="float-e-margins">
                <div class="ibox-title">
                    <h5>Listado de Historial de Puesto</h5>
                    <div class="ibox-tools">
                        <input type="button" id="btnAgregar" onclick="btnAgregar()" class="btn btn-primary btn-xs" value="Promover Empleado" />
                    </div>
                </div>
                <div class="ibox-content">
                    <table class="table table-striped table-bordered table-hover" id="IndexTable">
                        <thead>
                            <tr role="row">
                                <th>Encargado</th>
                                <th>Anterior</th>
                                <th>Nuevo</th>
                                <th>Fecha</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
@Html.Partial("_Create", new ERP_GMEDINA.Models.tbEmpleados())

@section Styles {
    @Styles.Render("~/Content/plugins/dataTables/dataTablesStyles")
    @Styles.Render("~/Content/plugins/izitoast/iziToast")
}

@section Scripts {
    @Scripts.Render("~/plugins/dataTablesMatutina")
    @Scripts.Render("~/Scripts/app/General")
    @Scripts.Render("~/Scripts/app/General/HistorialCargos")
}