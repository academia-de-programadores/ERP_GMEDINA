@model ERP_GMEDINA.Models.tbEquipoTrabajo

@{
    ViewBag.Title = "Equipo Trabajo ";
    Layout = "~/Views/Shared/_LayoutBase.cshtml";
}


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="float-e-margins">
                <div class="ibox-title">
                    <h5>Listado de Equipo de Trabajo</h5>
                    <div class="ibox-tools">
                        <input type="button" id="btnAgregar" class="btn btn-primary btn-xs" value="Registrar Equipo de Trabajo" />
                    </div>
                </div>
                <div class="ibox-content">
                  <table class="table table-striped table-bordered table-hover dataTables-example" id="IndexTable">
                    <thead>
                        <tr role="row">
                            <th>@Html.DisplayNameFor(model => model.eqtra_Codigo)</th>
                            <th>@Html.DisplayNameFor(model => model.eqtra_Descripcion)</th>
                            <th>@Html.DisplayNameFor(model => model.eqtra_Observacion)</th>
                            <th>Acciones</th>
                        </tr>
                    </thead>
                    <tbody></tbody>                       
                  </table>
                </div>
            </div>
        </div>
    </div>
 </div>
@Html.Partial("_Create", new ERP_GMEDINA.Models.tbEquipoTrabajo())
@Html.Partial("_Delete", new ERP_GMEDINA.Models.tbEquipoTrabajo())
@Html.Partial("_Edit", new ERP_GMEDINA.Models.tbEquipoTrabajo())
@Html.Partial("_Detalles", new ERP_GMEDINA.Models.tbEquipoTrabajo())
    
@section Styles {
    @Styles.Render("~/Content/plugins/dataTables/dataTablesStyles")
    @Styles.Render("~/Content/plugins/izitoast/iziToast")
}
@section Scripts {
@Scripts.Render("~/plugins/dataTables")
@Scripts.Render("~/Scripts/app/general")
@Scripts.Render("~/Scripts/app/general/EquipoTrabajo")
@if (Model.eqtra_Estado)
    {
    @Scripts.Render("~/Scripts/app/general/EquipoTrabajo/Admin")
    }
}
