@model IEnumerable<ERP_GMEDINA.Models.tbEquipoEmpleados>

@{
    ViewBag.Title = "Herramientas por Empleado";
    Layout = "~/Views/Shared/_LayoutBase.cshtml";
}

<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="float-e-margins">
                <div class="ibox-title">
                    <h5>Listado de Herramientas por Empleados</h5>
                </div>
                <div class="ibox-content">
                    <table class="table table-striped table-bordered table-hover dataTables-example" id="IndexTable">
                        <thead>
                            <tr role="row">
                                <th></th>
                                <th>Empleado</th>
                                <th>Correo</th>
                                <th>Telefono</th>
                            </tr>
                        </thead>
                        <tbody> </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

@Html.Partial("_Create", new ERP_GMEDINA.Models.tbEquipoEmpleados())
@Html.Partial("_Delete", new ERP_GMEDINA.Models.tbEquipoEmpleados())

@section Styles {
    @Styles.Render("~/Content/plugins/dataTables/dataTablesStyles")
    @Styles.Render("~/Content/plugins/izitoast/iziToast")
    @Styles.Render("~/Content/plugins/izitoast/iziToast.min")
}

@section Scripts {
@* datatble *@
    @Scripts.Render("~/Scripts/plugins/dataTablesMatutina/DatatablesConfigurations")
    @Scripts.Render("~/Scripts/plugins/dataTablesMatutina/datatables.min")

@*general*@
    @Scripts.Render("~/Scripts/app/general")
    @Scripts.Render("~/Scripts/plugins/iziToast/iziToast.min")
    @Scripts.Render("~/Scripts/plugins/iziToast/iziToast")
    @Scripts.Render("~/Scripts/app/General/EquipoEmpleados")
}
