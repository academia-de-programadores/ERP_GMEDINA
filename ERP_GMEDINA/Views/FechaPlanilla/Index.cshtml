@model IEnumerable<ERP_GMEDINA.Models.V_Plani_AnioPlanilla>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutBasePlanillas.cshtml";
}

<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Historial de Planillas</h5>
                </div>
                <div class="ibox-content">
                    <table id="tblHistorialPlanillas" class="table table-striped table-bordered table-hover">
                        <thead>
                            <tr>
                                <th></th>
                                <th>Fecha de Pago</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
@section Scripts {
    @Scripts.Render("~/plugins/dataTables")
    @Scripts.Render("~/Scripts/app/General/FechaPlanilla")
}

@section Styles {
    @Styles.Render("~/Content/app/General")
    @Styles.Render("~/Content/plugins/dataTables/dataTablesStyles")
}
