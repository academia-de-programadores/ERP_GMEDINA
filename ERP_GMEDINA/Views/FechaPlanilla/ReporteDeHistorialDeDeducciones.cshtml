@using ReportViewerForMvc;

@{
    ViewBag.Title = "Reporte Historial de deducciones";
    Layout = "~/Views/Shared/_LayoutBasePlanillas.cshtml";
}

<div class="row" style="">
    <div class="col-lg-12">
        <div class="float-e-margins">
            <div class="ibox-title">
                <h5>Previsualización de reporte Historial de Deducciones</h5>
                <div class="ibox-tools">
                    @Html.ActionLink("Regresar", "Index", null, new { @class = "btn btn-default btn-xs pull-right" })
                </div>
            </div>
            @{
                if (ViewBag.ReportViewerDeducciones != null)
                {
                    <div class="ibox-content">
                        @Html.ReportViewer(ViewBag.ReportViewerDeducciones as Microsoft.Reporting.WebForms.ReportViewer)
                    </div>
                }
            }
        </div>
    </div>
</div>