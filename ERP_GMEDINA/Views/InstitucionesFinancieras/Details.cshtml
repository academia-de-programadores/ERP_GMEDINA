@model ERP_GMEDINA.Models.tbInstitucionesFinancieras

@{
    ViewBag.Title = "Detalles";
    Layout = "~/Views/Shared/_LayoutBase.cshtml";
}

<div class="col-lg-12">
    <div class="ibox float-e-margins">
        <div class="ibox-title">
            @Html.ActionLink("Regresar", "Index", null, new { @class = "pull-right" })
            <h2>Detalles de Regsitro</h2>
        </div>

        <div class="ibox-content">
            <dl class="dl-horizontal">
                <dt >
                    @Html.LabelFor(model => model.insf_DescInstitucionFinanc, htmlAttributes: new { @width = "100" })
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.insf_DescInstitucionFinanc)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.insf_Contacto)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.insf_Contacto)
                </dd>

                <dt>
                    @Html.DisplayNameFor(model => model.insf_Telefono)
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.insf_Telefono)
                </dd>

                <dt>
                    @Html.LabelFor(model => model.insf_Correo, htmlAttributes: new { @width = 150 })
                </dt>

                <dd>
                    @Html.DisplayFor(model => model.insf_Correo)
                </dd>
            </dl>
        </div>
    </div>



    <div class="ibox float-e-margins">
        <div class="ibox-title">
            <h3>Auditoría</h3>
        </div>
        <div class="ibox-content">
            <div class="form-horizontal">
                <table class="table table-striped table-bordered dataTable">
                    <thead>
                        <tr>
                            <th>Acción</th>
                            <th>Usuario</th>
                            <th>Fecha</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>Creado</td>
                            <td>@Html.DisplayFor(model => model.insf_UsuarioCrea)</td>
                            <td>@Html.DisplayFor(model => model.insf_FechaCrea)</td>
                        </tr>
                        <tr>
                            <td>Modificado</td>
                            <td>@Html.DisplayFor(model => model.insf_UsuarioModifica)</td>
                            <td>@Html.DisplayFor(model => model.insf_FechaModifica)</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>








</div>
