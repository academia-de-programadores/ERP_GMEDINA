@model ERP_GMEDINA.Models.tbSeleccionCandidatos
<div class="modal inmodal" id="ModalDetalles" tabindex="-1" role="dialog" style="display: none;" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content animated fadeIn">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title">Detalle SeleccionCandidatos</h4>
            </div>
            <div class="modal-body">
                <dl class="dl-horizontal">
                    <dt>@Html.DisplayNameFor(model => model.tbPersonas.per_Identidad)</dt>
                    <dd id="tbPersonas"></dd>

                    <dt>Nombre</dt>
                    <dd id="tbPersonasNombre"></dd>

                    <dt>@Html.DisplayNameFor(model => model.tbFasesReclutamiento.fare_Descripcion)</dt>
                    <dd id="tbFasesReclutamiento"></dd>

                    <dt>@Html.DisplayNameFor(model => model.tbRequisiciones.req_Descripcion)</dt>
                    <dd id="tbRequisiciones"></dd>

					<dt>@Html.DisplayNameFor(model => model.scan_Fecha)</dt>
                    <dd id="scan_Fecha"></dd>

                    <dt>@Html.DisplayNameFor(model => model.tbUsuario.usu_NombreUsuario)</dt>
                    <dd id="tbUsuario_usu_NombreUsuario"></dd>

					<dt>@Html.DisplayNameFor(model => model.scan_FechaCrea)</dt>
                    <dd id="scan_FechaCrea"></dd>

                    <dt>@Html.DisplayNameFor(model => model.tbUsuario1.usu_NombreUsuario)</dt>
                    <dd id="tbUsuario1_usu_NombreUsuario"></dd>	

					<dt>@Html.DisplayNameFor(model => model.scan_FechaModifica)</dt>
                    <dd id="scan_FechaModifica"></dd>			
				
				</dl>
            </div>
			<div class="modal-footer">
				<button type="button" class="btn btn-white pull-right" data-dismiss="modal">Cerrar</button>
				<button type="button" class="btn btn-primary pull-left" data-id="" id="btnEditar">Editar</button>
			</div>
		</div>
	</div>
</div>

