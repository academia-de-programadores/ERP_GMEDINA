@model ERP_GMEDINA.Models.tbHistorialVacaciones

<div class="modal inmodal" id="ModalDetalles" tabindex="-1" role="dialog" style="display: none;" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content animated fadeIn">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title">Detalle HistorialVacaciones</h4>
            </div>
            <div class="modal-body">
                <dl class="dl-horizontal">
                    <dt>@Html.DisplayNameFor(model => model.hvac_FechaInicio)</dt>
                    <dd id="hvac_FechaInicio"></dd>

                    <dt>@Html.DisplayNameFor(model => model.hvac_FechaFin)</dt>
                    <dd id="hvac_FechaFin"></dd>

                    <dt>@Html.DisplayNameFor(model => model.hvac_CantDias)</dt>
                    <dd id="hvac_CantDias"></dd>

                    <dt>@Html.DisplayNameFor(model => model.hvac_DiasPagados)</dt>
                    <dd id="hvac_DiasPagados"></dd>

                    <dt>@Html.DisplayNameFor(model => model.hvac_MesVacaciones)</dt>
                    <dd id="hvac_MesVacaciones"></dd>

                    <dt>@Html.DisplayNameFor(model => model.hvac_AnioVacaciones)</dt>
                    <dd id="hvac_AnioVacaciones"></dd>

                    <dt>@Html.DisplayNameFor(model => model.hvac_Estado)</dt>
                    <dd id="hvac_Estado"></dd>

                    <dt>@Html.DisplayNameFor(model => model.hvac_RazonInactivo)</dt>
                    <dd id="hvac_RazonInactivo"></dd>

                    <dt>@Html.DisplayNameFor(model => model.hvac_FechaCrea)</dt>
                    <dd id="hvac_FechaCrea"></dd>

                    <dt>@Html.DisplayNameFor(model => model.hvac_FechaModifica)</dt>
                    <dd id="hvac_FechaModifica"></dd>

                    <dt>@Html.DisplayNameFor(model => model.hvac_UsuarioCrea)</dt>
                    <dd id="hvac_UsuarioCrea"></dd>

                    <dt>@Html.DisplayNameFor(model => model.hvac_UsuarioModifica)</dt>
                    <dd id="hvac_UsuarioModifica"></dd>



                </dl>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-white pull-right" data-dismiss="modal">Cerrar</button>
                <button type="button" class="btn btn-primary pull-left" data-id="" id="btnEditar">Editar</button>
            </div>
        </div>
    </div>
</div>

