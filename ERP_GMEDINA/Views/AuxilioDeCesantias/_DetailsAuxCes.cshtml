@model ERP_GMEDINA.Models.tbAuxilioDeCesantias
@using (Html.BeginForm(null, null, FormMethod.Post, new { id = "frmDetallesAuxCess" }))
{
<div class="ibox-content">

    <div class="form-horizontal">
        <table class="table no-borders">
            <thead>
                <tr>
                    <th>Descripcion</th>
                    <th>Valor</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>Código Auxilio Cesantia)</td>
                    <td>@Html.DisplayFor(model => model.aces_IdAuxilioCesantia, new { htmlAttributes = new { @class = "text-box single-line", @disabled = "disabled" } })</td>
                </tr>
                <tr>
                    <td>Rango Inicio (Meses)</td>
                    <td>@Html.DisplayFor(model => model.aces_RangoInicioMeses, new { htmlAttributes = new { @class = "text-box single-line", @disabled = "disabled" } })</td>
                </tr>
                <tr>
                    <td> Rango Fin (Meses)</td>
                    <td>@Html.DisplayFor(model => model.aces_RangoFinMeses, new { htmlAttributes = new { @class = "text-box single-line", @disabled = "disabled" } })</td>
                </tr>
                <tr>
                    <td> Auxilio Cesantia (Dias)</td>
                    <td>@Html.DisplayFor(model => model.aces_DiasAuxilioCesantia, new { htmlAttributes = new { @class = "text-box single-line", @disabled = "disabled" } })</td>
                </tr>
            </tbody>
        </table>
    </div>
</div>
<div class="ibox float-e-margins">
    <div class="ibox-title">
        <h4>Auditoría</h4>
    </div>
    <div class="ibox-content">
        <div class="form-horizontal">
            <table class="table table-striped table-bordered dataTable">
                <thead>
                    <tr>
                        <th>Acción</th>
                        <th>Usuario</th>
                        <th>Fecha</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>Creado</td>
                        @Html.HiddenFor(model => model.aces_UsuarioCrea, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })
                        <td>@Html.EditorFor(model => model.tbUsuario.usu_NombreUsuario, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</td>
                        <td>@Html.EditorFor(model => model.aces_FechaCrea, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</td>
                    </tr>
                    <tr>
                        <td>Modificado</td>
                        @Html.HiddenFor(model => model.aces_UsuarioModifica, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })
                        <td>@Html.EditorFor(model => model.tbUsuario1.usu_NombreUsuario, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</td>
                        <td>@Html.EditorFor(model => model.aces_FechaModifica, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>
}

