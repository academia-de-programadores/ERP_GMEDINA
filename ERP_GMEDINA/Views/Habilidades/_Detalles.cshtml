@model ERP_GMEDINA.Models.tbHabilidades
<div class="modal inmodal" id="ModalDetalles" tabindex="-1" role="dialog" style="display: none;" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content animated fadeIn">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title">Editar habilidad</h4>
            </div>
            <div class="modal-body">
                <div class="ibox-content">
<div class="ibox-content">
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.habi_Descripcion)
        </dt>

        <dd id="habi_Descripcion">
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.habi_Estado)
        </dt>

        <dd id="habi_Estado">
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.habi_RazonInactivo)
        </dt>

        <dd id="habi_RazonInactivo">
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.habi_FechaCrea)
        </dt>

        <dd id="habi_FechaCrea">
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.habi_FechaModifica)
        </dt>

        <dd id="habi_FechaModifica">
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.tbUsuario.usu_NombreUsuario)
        </dt>

        <dd id="tbUsuario_usu_NombreUsuario">
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.tbUsuario1.usu_NombreUsuario)
        </dt>

        <dd id="tbUsuario1_usu_NombreUsuario">
        </dd>

    </dl>
</div>
</div>
</div>
            <div class="modal-footer">
                <button type="button" class="btn btn-white pull-right" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary pull-left" data-id="" id="btnEditar">Editar</button>
            </div>
        </div>
    </div>
</div>
