@model ERP_GMEDINA.Models.tbISR
@using (Html.BeginForm(null, null, FormMethod.Post, new { id = "frmDetailsISR" }))
{ 
@Html.AntiForgeryToken()
<div class="form-horizontal">
    <div class="form-group">
        <label class="control-label col-md-2">Rango Inicial</label>
        <div class="col-md-10">
            @Html.EditorFor(model => model.isr_RangoInicial, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
            @Html.ValidationMessageFor(model => model.isr_RangoInicial, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group">
        <label class="control-label col-md-2">Rango Final</label>
        <div class="col-md-10">
            @Html.EditorFor(model => model.isr_RangoFinal, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
            @Html.ValidationMessageFor(model => model.isr_RangoFinal, "", new { @class = "text-danger" })
        </div>
    </div>
    <div class="form-group">
        <label class = "control-label col-md-2">Porcentaje</label>
        <div class="col-md-10">
            @Html.EditorFor(model => model.isr_Porcentaje, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
            @Html.ValidationMessageFor(model => model.isr_Porcentaje, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group">
        <label class="control-label col-md-2">Tipo Deducción</label>
        <div class="col-md-10">
            <select id="tde_IdTipoDedu" name="tde_IdTipoDedu" class="form-control" disabled="disabled"></select>
        </div>
    </div>
</div>
<div class="ibox float-e-margins">
    <div class="ibox-title">
        <h2>Auditoría</h2>
    </div>
    <div class="ibox-content">
        <div class="form-horizontal">
            <table class="table table-striped table-bordered dataTable">
                <thead>
                    <tr>
                        <th>Acción</th>
                        <th>Usuario</th>
                        <th>Fecha</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>Creado</td>
                        @Html.HiddenFor(model => model.isr_UsuarioCrea, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })
                        <td>@Html.EditorFor(model => model.tbUsuario.usu_NombreUsuario, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</td>
                        <td>@Html.EditorFor(model => model.isr_FechaCrea, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</td>
                    </tr>
                    <tr>
                        <td>Modificado</td>
                        @Html.HiddenFor(model => model.isr_UsuarioModifica, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })
                        <td>@Html.EditorFor(model => model.tbUsuario1.usu_NombreUsuario, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</td>
                        <td>@Html.EditorFor(model => model.isr_FechaModifica, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>
}