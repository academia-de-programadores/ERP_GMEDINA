@model ERP_GMEDINA.Models.tbTipoSalidas
<div class="modal inmodal" id="ModalDetalles" tabindex="-1" role="dialog" style="display: none;" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content animated fadeIn">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span><span class="sr-only">Cerrar</span></button>
                <h4 class="modal-title">Detalles Tipo de Salida</h4>
            </div>
            <div class="modal-body">

                        <dl class="dl-horizontal">
                            <dt>
                                @Html.DisplayNameFor(model => model.tsal_Descripcion)
                            </dt>
                            <dd id="tsal_Descripcion">
                            </dd>
                        </dl>
                <div class="ibox-content">
                        <div class="ibox float-e-margins">
                            <div class="ibox-title">
                                <h4>Auditoría</h4>
                            </div>
                            <div class="ibox-content">
                                <div class="form-horizontal">
                                    <table class="table table-striped table-bordered dataTable">
                                        <thead>
                                            <tr>
                                                <th>Acción</th>
                                                <th>Usuario</th>
                                                <th>Fecha</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <td>Creado</td>
                                                <td><span id="tbUsuario_usu_NombreUsuario">@Html.DisplayFor(model => model.tbUsuario.usu_NombreUsuario, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</span></td>
                                                <td><span id="tsal_FechaCrea">@Html.DisplayFor(model => model.tsal_FechaCrea, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</span></td>
                                            </tr>
                                            <tr>
                                                <td>Modificado</td>
                                                <td><span id="tbUsuario1_usu_NombreUsuario">@Html.DisplayFor(model => model.tbUsuario1.usu_NombreUsuario, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</span></td>
                                                <td><span id="tsal_FechaModifica">@Html.DisplayFor(model => model.tsal_FechaModifica, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</span></td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>

                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-white pull-right" data-dismiss="modal">Cerrar</button>
                @*<button type="button" class="btn btn-primary pull-left" data-id="" id="btnEditar">Editar</button>*@
            </div>
        </div>
    </div>
</div>
