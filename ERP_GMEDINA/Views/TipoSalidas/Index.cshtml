@model ERP_GMEDINA.Models.tbTipoSalidas

@{
    ViewBag.Title = "Tipos de Salidas";
    Layout = "~/Views/Shared/_LayoutBaseRRHH.cshtml";
}
<div class="wrapper wrapper-content animated fadeInRight">
 <div class="row">
  <div class="col-lg-12">
   <div class="float-e-margins">
    <div class="ibox-title">
     <h5>Listado de Tipos de Salidas</h5>
     <div class="ibox-tools">
      <input type="button" id="btnAgregar" class="btn btn-primary btn-xs" value="Registrar un Tipo de Salida" />
     </div>
    </div>
    <div class="ibox-content">
     <table class="table table-striped table-bordered table-hover dataTables-example" id="IndexTable">
      <thead>
       <tr role="row">
        <th>Número</th>
        <th>@Html.DisplayNameFor(model => model.tsal_Descripcion)</th>
        <th>@Html.DisplayNameFor(model => model.tsal_Estado)</th>
       <th>Acciones</th>
      </tr>
     </thead>
     <tbody></tbody>
    </table>
   </div>
  </div>
 </div>
</div>
</div>
@Html.Partial("_Activar")
@Html.Partial("_Create", new ERP_GMEDINA.Models.tbTipoSalidas { })
@Html.Partial("_Delete", new ERP_GMEDINA.Models.tbTipoSalidas { })
@Html.Partial("_Edit", new ERP_GMEDINA.Models.tbTipoSalidas { })
@Html.Partial("_Detalles", new ERP_GMEDINA.Models.tbTipoSalidas { })
@section Styles {
@Styles.Render("~/Content/plugins/dataTables/dataTablesStyles")
@Styles.Render("~/Content/plugins/izitoast/iziToast")
@Styles.Render("~/Content/plugins/izitoast/iziToast.min")
}

@section Scripts {
@* datatble *@
@Scripts.Render("~/Scripts/plugins/dataTablesMatutina/DatatablesConfigurations")
@Scripts.Render("~/Scripts/plugins/dataTablesMatutina/datatables.min")

@*general*@
@Scripts.Render("~/Scripts/app/general")
@Scripts.Render("~/Scripts/plugins/iziToast/iziToast.min")
@Scripts.Render("~/Scripts/plugins/iziToast/iziToast")
@Scripts.Render("~/Scripts/app/general/TipoSalidas")
@if ((bool)Session["Admin"])
{
    @Scripts.Render("~/Scripts/app/general/TipoSalidas/Admin")
}
}
