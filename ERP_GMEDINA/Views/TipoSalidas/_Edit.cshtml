@model ERP_GMEDINA.Models.tbTipoSalidas
<div class="modal inmodal" id="ModalEditar" tabindex="-1" role="dialog" style="display: none;" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content animated fadeIn">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span><span class="sr-only">Cerrar</span></button>
                <h4 class="modal-title">Editar Tipo de salida</h4>
            </div>
            <div class="modal-body">
               
                    @using (Html.BeginForm(null, null, FormMethod.Post, new { id = "FormEditar" }))
                    {
                      
                            <div class="form-horizontal">
                                <div class="form-group">
                                    @Html.LabelFor(model => model.tsal_Descripcion, htmlAttributes: new { @class = "control-label col-md-2" })
                                    <div class="col-md-10">
                                        @Html.EditorFor(model => model.tsal_Descripcion, new { htmlAttributes = new { @class = "form-control required valid", maxlength = 50, @onkeypress = "return alphanumeric(event)" } })
                                        @Html.ValidationMessageFor(model => model.tsal_Descripcion, "", new { @class = "text-danger", @id = "errortsal_Descripcion" })
                                    </div>
                                </div>
                            </div>
                       
                    }
               
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-white pull-right" data-dismiss="modal">Cerrar</button>
                <button type="button" class="btn btn-primary pull-left" id="btnActualizar">Editar</button>
                <button type="button" class="btn btn-danger pull-left" id="btnInactivar">Inactivar</button>
            </div>
        </div>
    </div>
</div>