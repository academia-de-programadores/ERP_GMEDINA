@model IEnumerable<ERP_GMEDINA.Models.tbTitulos>

@{
    ViewBag.Title = "Titulos";
    Layout = "~/Views/Shared/_LayoutBase.cshtml";
}


<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="float-e-margins">
                <div class="ibox-title">
                    <h5>Listado de Titulos</h5>
                    <div class="ibox-tools">
                        <input type="button" id="btnAgregar" class="btn btn-primary btn-xs" value="Registrar Titulo" />
                    </div>
                </div>
                <div class="ibox-content">
                  <table class="table table-striped table-bordered table-hover dataTables-example" id="IndexTable">
                    <thead>
                        <tr role="row">
                            <th>@Html.DisplayNameFor(model => model.titu_Descripcion)</th>
                            <th>Acciones</th>
                        </tr>
                    </thead>
                    <tbody>
            
                    </tbody>                       
                  </table>
                </div>
            </div>
        </div>
    </div>
 </div>
@Html.Partial("_Create", new ERP_GMEDINA.Models.tbTitulos())
@Html.Partial("_Delete", new ERP_GMEDINA.Models.tbTitulos())
@Html.Partial("_Edit", new ERP_GMEDINA.Models.tbTitulos())
@Html.Partial("_Detalles", new ERP_GMEDINA.Models.tbTitulos())
    
@section Styles {
    @Styles.Render("~/Content/plugins/dataTables/dataTablesStyles")
    @Styles.Render("~/Content/plugins/izitoast/iziToast")
}
@section Scripts {
@Scripts.Render("~/plugins/dataTables")
@Scripts.Render("~/Scripts/app/general")
@Scripts.Render("~/Scripts/app/General/Titulos")
}
