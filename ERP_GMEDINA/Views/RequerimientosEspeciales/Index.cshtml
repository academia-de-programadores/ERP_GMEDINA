@model ERP_GMEDINA.Models.tbRequerimientosEspeciales

@{
    ViewBag.Title = "Requerimientos Especiales";
    Layout = "~/Views/Shared/_LayoutBase.cshtml";
}



<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="float-e-margins">
                <div class="ibox-title">
                    <h5>Listado de Requerimientos Especiales</h5>
                    <div class="ibox-tools">
                        <input type="button" id="btnAgregar" class="btn btn-primary btn-xs" value="Registrar Requerimiento Especial" />
                    </div>
                </div>
                <div class="ibox-content">
                  <table class="table table-striped table-bordered table-hover dataTables-example" id="IndexTable">
                    <thead>
                        <tr role="row">
                            <th>Número</th>
                            <th>@Html.DisplayNameFor(model => model.resp_Descripcion)</th>
                            <th>Estado</th>
                            <th>Acciones</th>
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>                       
                  </table>
                </div>
            </div>
        </div>
    </div>
 </div>


<div class="modal fade" id="ModalHabilitar" tabindex="-1" role="dialog" aria-hidden="false">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Cerrar</span></button>
                <h4 class="modal-title">Activar</h4>
            </div>
            <div class="modal-body" id="Activar">
                ¿Esta seguro que desea Activar este registro?
            </div>
            <input type="hidden" value="" id="txtIdRestore">
            <div class="modal-footer">
                <button type="button" class="btn btn-white pull-right" data-dismiss="modal">No</button>
                <button type="button" class="btn btn-primary pull-right" id="btnActivar" data-dismiss="modal">Si</button>
                <div style="display:none" class="btn pull-right" id="cargandoActivar"></div>
            </div>
        </div>
    </div>
</div>
@Html.Partial("_Create", new ERP_GMEDINA.Models.tbRequerimientosEspeciales())
@Html.Partial("_Delete", new ERP_GMEDINA.Models.tbRequerimientosEspeciales())
@Html.Partial("_Edit", new ERP_GMEDINA.Models.tbRequerimientosEspeciales())
@Html.Partial("_Detalles", new ERP_GMEDINA.Models.tbRequerimientosEspeciales())
    



@section Styles {
    @Styles.Render("~/Content/plugins/dataTables/dataTablesStyles")
    @Styles.Render("~/Content/plugins/izitoast/iziToast")
}
@section Scripts {
    @Scripts.Render("~/plugins/dataTablesMatutina")
    @Scripts.Render("~/Scripts/app/general")
    @Scripts.Render("~/Scripts/app/General/RequerimientosEspeciales")

    @if (Model.resp_Estado)
    {
        @Scripts.Render("~/Scripts/app/General/RequerimientosEspeciales/Admin")
    }
}


