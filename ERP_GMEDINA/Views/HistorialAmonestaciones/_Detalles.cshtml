@model ERP_GMEDINA.Models.tbHistorialAmonestaciones
<div class="modal inmodal" id="ModalDetalles" tabindex="-1" role="dialog" style="display: none;" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content animated fadeIn">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title">Detalle Historial Amonestaciones</h4>
            </div>
            <div class="modal-body">
                <dl class="dl-horizontal">
                    <dt>@Html.DisplayNameFor(model => model.emp_Id)</dt>
                    <dd id="emp_Id"></dd>

					<dt>@Html.DisplayNameFor(model => model.hamo_Observacion)</dt>
                    <dd id="hamo_Observacion"></dd>

                    <dt>@Html.DisplayNameFor(model => model.tbTipoAmonestaciones.tamo_Descripcion)</dt>
                      <dd id="tbTipoAmonestaciones_tamo_Descripcion"></dd>

					<dt>@Html.DisplayNameFor(model => model.hamo_Fecha)</dt>
                    <dd id="hamo_Fecha"></dd>

					<dt>@Html.DisplayNameFor(model => model.tbUsuario.usu_NombreUsuario)</dt>
                    <dd id="tbUsuario_usu_NombreUsuario"></dd>

                    <dt>@Html.DisplayNameFor(model => model.hamo_FechaCrea)</dt>
                    <dd id="hamo_FechaCrea"></dd>
				</dl>
            </div>
			<div class="modal-footer">
				<button type="button" class="btn btn-white pull-right" data-dismiss="modal">Cerrar</button>
			</div>
		</div>
	</div>
</div>

