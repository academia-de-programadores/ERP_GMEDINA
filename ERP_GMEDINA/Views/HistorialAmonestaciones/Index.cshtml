@model IEnumerable<ERP_GMEDINA.Models.tbHistorialAmonestaciones>

@{
    ViewBag.Title = "Historial Amonestaciones";
    Layout = "~/Views/Shared/_LayoutBase.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Historial de Amonestaciones</h2>
        <ol class="breadcrumb">
            <li class="active">
            </li>
        </ol>
    </div>
</div>

<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="float-e-margins">
                <div class="ibox-title">
                    <h5>Listado de tbHistorialAmonestaciones</h5>
                    <div class="ibox-tools">
                       @Html.ActionLink("Registrar tbHistorialAmonestaciones", "Create", null, new { @class = "btn btn-primary btn-xs"})
