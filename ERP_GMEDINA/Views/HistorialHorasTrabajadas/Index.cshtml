@model IEnumerable<ERP_GMEDINA.Models.tbHistorialHorasTrabajadas>

@{
    ViewBag.Title = "Historial Horas Trabajadas";
    Layout = "~/Views/Shared/_LayoutBase.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>Historial de Horas Trabajadas</h2>
        <ol class="breadcrumb">
            <li class="active">
            </li>
        </ol>
    </div>
</div>

<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="float-e-margins">
                <div class="ibox-title">
                    <h5>Listado de HistorialHorasTrabajadas</h5>
                    <div class="ibox-tools">
                        <input type="button" id="btnAgregar" class="btn btn-primary btn-xs" value="Registrar HistorialHorasTrabajadas" />
                    </div>
                </div>
                <div class="ibox-content">
                  <table class="table table-striped table-bordered table-hover dataTables-example" id="IndexTable">
                    <thead>
                        <tr role="row">
                            <th></th>
                            <th>@Html.DisplayNameFor(model => model.tbPersonas.per_Nombres) </th>
                            <th>@Html.DisplayNameFor(model => model.tbPersonas.per_Apellidos)  </th>
                            <th>@Html.DisplayNameFor(model => model.tbJornadas.jor_Descripcion)</th>
                            <th>@Html.DisplayNameFor(model => model.tbTipoHoras.tiho_Descripcion)</th>
                            <th>@Html.DisplayNameFor(model => model.tbTipoHoras.tiho_Recargo)</th>                               
                        </tr>
                    </thead>
                    <tbody></tbody>                       
                  </table>
                </div>
            </div>
        </div>
    </div>
 </div>

    
@section Styles {
    @Styles.Render("~/Content/plugins/dataTables/dataTablesStyles")
    @Styles.Render("~/Content/plugins/izitoast/iziToast")
}
@section Scripts {
@Scripts.Render("~/plugins/dataTables")
@Scripts.Render("~/Scripts/app/general")
@Scripts.Render("~/Scripts/app/general/HistorialHorasTrabajadas")
}
