@model ERP_GMEDINA.Models.tbCatalogoDeIngresos
@using (Html.BeginForm(null, null, FormMethod.Post, new { id = "frmCatalogoIngresos" }))
{
    @Html.AntiForgeryToken()

    @*<div class="ibox-content">*@
    <div class="form-horizontal">
        <div class="row">

            @Html.HiddenFor(model => model.cin_IdIngreso)
            @Html.HiddenFor(model => model.cin_FechaCrea)
            @Html.HiddenFor(model => model.cin_UsuarioCrea)
            <div class="col-md-6">
                @Html.LabelFor(model => model.cin_IdIngreso, new { htmlAttributes = new { @class = "text-box single-line", @disabled = "disabled" } })
            </div>

            <div class="col-md-6">
                <span id="cin_IdIngreso">
                    @Html.DisplayFor(model => model.cin_IdIngreso, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                </span>
                @Html.ValidationMessageFor(model => model.cin_IdIngreso, "", new { @class = "text-danger" })
            </div>
        </div>


        <div class="row">
            <div class="col-md-6">
                @Html.LabelFor(model => model.cin_DescripcionIngreso, new { htmlAttributes = new { @class = "text-box single-line", @disabled = "disabled" } })
            </div>
            <div class="col-md-6">
                <span id="cin_DescripcionIngreso">
                    @Html.DisplayFor(model => model.cin_DescripcionIngreso, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                </span>
                @Html.ValidationMessageFor(model => model.cin_DescripcionIngreso, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="row">
            <div class="col-md-6">
                <label class="">Tipo de Ingreso</label>
            </div>
            <div class="col-md-6">
                <span id="tipoDeIngresoDetalle">Valor</span>
            </div>
        </div>
    </div>
    <br />
            @*</div>*@
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h4>Auditoría</h4>
                </div>
                <div class="ibox-content">
                    <div class="form-horizontal">
                        <table class="table table-striped table-bordered dataTable">
                            <thead>
                                <tr>
                                    <th>Acción</th>
                                    <th>Usuario</th>
                                    <th>Fecha</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>Creado</td>
                                    @Html.HiddenFor(model => model.cin_UsuarioCrea, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })
                                    <td><span id="tbUsuario_usu_NombreUsuario">@Html.DisplayFor(model => model.tbUsuario.usu_NombreUsuario, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</span></td>
                                    <td><span id="cin_FechaCrea">@Html.DisplayFor(model => model.cin_FechaCrea, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</span></td>
                                </tr>
                                <tr>
                                    <td>Modificado</td>
                                    @Html.HiddenFor(model => model.cin_UsuarioModifica, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })
                                    <td><span id="tbUsuario1_usu_NombreUsuario">@Html.EditorFor(model => model.tbUsuario1.usu_NombreUsuario, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</span></td>
                                    <td><span id="cin_FechaModifica">@Html.EditorFor(model => model.cin_FechaModifica, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</span></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                    <div class="footer">
                        <button type="button" class="btn btn-white pull-right" data-dismiss="modal">Cerrar</button>
                    </div>
                </div>
            </div>
}
