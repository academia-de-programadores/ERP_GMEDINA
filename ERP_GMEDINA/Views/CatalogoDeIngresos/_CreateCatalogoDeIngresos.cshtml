@model ERP_GMEDINA.Models.tbCatalogoDeIngresos
@using (Html.BeginForm(null, null, FormMethod.Post, new { id = "frmCatalogoIngresosCreate" }))
{
    @Html.AntiForgeryToken()
    <div class="form-horizontal">
        @Html.HiddenFor(model => model.cin_IdIngreso)
        @Html.HiddenFor(model => model.cin_FechaCrea)
        @Html.HiddenFor(model => model.cin_UsuarioCrea)
        <div class="form-group">
            @Html.LabelFor(model => model.cin_DescripcionIngreso, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.cin_DescripcionIngreso, new { htmlAttributes = new { @class = "form-control", id= "cin_DescripcionIngreso", @autocomplete = "off" } })
                @Html.ValidationMessageFor(model => model.cin_DescripcionIngreso, "", new { @class = "text-danger" ,@id="descripcioncrear"})
            </div>
        </div>
    </div>
    <div class="modal-footer">
        <button type="button" data-dismiss="modal" class="btn btn-white pull-right" id="btnCerrarCrear">Cerrar</button>
    <input type="submit" id="btnCreateRegistroIngresos" value="Guardar" class="btn btn-primary" />
    <div style="display:none" class="btn" id="cargandoCrear">
    </div>
</div>


}
