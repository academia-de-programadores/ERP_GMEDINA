@model ERP_GMEDINA.Models.tbTechosDeducciones
@using (Html.BeginForm(null, null, FormMethod.Post, new { id = "frmDetailsTechosDeducciones" }))
{ 
@Html.AntiForgeryToken()
<div class="form-horizontal">
    <div class="form-group">
        <label class="control-label col-md-2">Deducción</label>
        <div class="col-md-10">
            <select id="cde_IdDeducciones" name="cde_IdDeducciones" class="form-control" disabled="disabled"></select>
        </div>
    </div>
    <div class="form-group">
        <label class="control-label col-md-2">Techo</label>
        <div class="col-md-10">
            @Html.EditorFor(model => model.tddu_Techo, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
            @Html.ValidationMessageFor(model => model.tddu_Techo, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group">
        <label class="control-label col-md-2">% colaboradores</label>
        <div class="col-md-10">
            @Html.EditorFor(model => model.tddu_PorcentajeColaboradores, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
            @Html.ValidationMessageFor(model => model.tddu_PorcentajeColaboradores, "", new { @class = "text-danger" })
        </div>
    </div>
    <div class="form-group">
        <label class = "control-label col-md-2">% empresa</label>
        <div class="col-md-10">
            @Html.EditorFor(model => model.tddu_PorcentajeEmpresa, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
            @Html.ValidationMessageFor(model => model.tddu_PorcentajeEmpresa, "", new { @class = "text-danger" })
        </div>
    </div>

    
</div>
<div class="ibox float-e-margins">
    <div class="ibox-title">
        <h2>Auditoría</h2>
    </div>
    <div class="ibox-content">
        <div class="form-horizontal">
            <table class="table table-striped table-bordered dataTable">
                <thead>
                    <tr>
                        <th>Acción</th>
                        <th>Usuario</th>
                        <th>Fecha</th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        <td>Creado</td>
                        @Html.HiddenFor(model => model.tddu_UsuarioCrea, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })
                        <td>@Html.EditorFor(model => model.tbUsuario.usu_NombreUsuario, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</td>
                        <td>@Html.EditorFor(model => model.tddu_FechaCrea, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</td>
                    </tr>
                    <tr>
                        <td>Modificado</td>
                        @Html.HiddenFor(model => model.tddu_UsuarioModifica, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })
                        <td>@Html.EditorFor(model => model.tbUsuario1.usu_NombreUsuario, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</td>
                        <td>@Html.EditorFor(model => model.tddu_FechaModifica, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</div>
}