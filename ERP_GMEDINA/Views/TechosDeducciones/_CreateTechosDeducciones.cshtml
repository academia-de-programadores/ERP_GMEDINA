@model ERP_GMEDINA.Models.tbTechosDeducciones
@using (Html.BeginForm(null, null, FormMethod.Post, new { id = "frmTechosDeduccionesCreate" }))
{
    @Html.AntiForgeryToken()
    <div class="form-horizontal">
        @Html.HiddenFor(model => model.tede_Id)
        @Html.HiddenFor(model => model.tede_FechaCrea)
        @Html.HiddenFor(model => model.tede_UsuarioCrea)
        <div class="form-group">
            @Html.LabelFor(model => model.tede_RangoInicial, htmlAttributes: new { @class = "control-label col-md-3" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.tede_RangoInicial, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.tede_RangoInicial, "", new { @class = "text-danger" })
            </div>
        </div>

        <div class="form-group">
        @Html.LabelFor(model => model.tede_RangoFinal, htmlAttributes: new { @class = "control-label col-md-3" })
        <div class="col-md-10">
            @Html.EditorFor(model => model.tede_RangoFinal, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.tede_RangoFinal, "", new { @class = "text-danger" })
        </div>
    </div>

        <div class="form-group">
            @Html.LabelFor(model => model.cde_IdDeducciones, htmlAttributes: new { @class = "control-label col-md-3" })
            <div class="col-md-10">
                <select id="cde_IdDeducciones" name="cde_IdDeducciones" class="form-control"></select>
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.tede_Porcentaje, htmlAttributes: new { @class = "control-label col-md-3" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.tede_Porcentaje, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.tede_Porcentaje, "", new { @class = "text-danger" })
            </div>
        </div>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-white" data-dismiss="modal">Cerrar</button>
        <input type="submit" class="btn btn-primary" id="btnCreateTechoDeducciones" value="Aceptar">
    </div>
}
