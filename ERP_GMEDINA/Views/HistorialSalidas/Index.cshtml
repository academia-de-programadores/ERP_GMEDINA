@model IEnumerable<ERP_GMEDINA.Models.tbHistorialSalidas>

@{
        /**/

        ViewBag.Title = "Index";
        Layout = "~/Views/Shared/_LayoutBase.cshtml";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-sm-4">
        <h2>tbHistorialSalidas</h2>
        <ol class="breadcrumb">
            <li class="active">
            </li>
        </ol>
    </div>
</div>

<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="float-e-margins">
                <div class="ibox-title">
                    <h5>Listado de tbHistorialSalidas</h5>
                    <div class="ibox-tools">
                        @Html.ActionLink("Registrar tbHistorialSalidas", "Create", null, new { @class = "btn btn-primary btn-xs" })
                    </div>
                </div>
                <div class="ibox-content">
                    <table class="table table-striped table-bordered table-hover dataTables-example" id="IndexTable">
                        <thead>
                            <tr role="row">
                                <th></th>
                                <th>Nombre Completo</th>
                                <th>Tipo Salida</th>
                                <th>Acciones</th>
                            </tr>
                        </thead>
                        <tbody>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

@section Styles {
    @Styles.Render("~/Content/plugins/dataTables/dataTablesStyles")
}
@section Scripts {
    @Scripts.Render("~/Scripts/app/general")
    @Scripts.Render("~/plugins/dataTables")
    @Scripts.Render("~/Scripts/app/general/IndexHistorialSalidas")
}
