@model ERP_GMEDINA.Models.tbDeduccionAFP
@using (Html.BeginForm())
{
    <div class="ibox-content">
        <div class="form-horizontal">
            @Html.AntiForgeryToken()
            @Html.HiddenFor(model => model.dafp_Id)

            <div class="row">
                <div class="col-md-6">
                    @Html.LabelFor(model => model.emp_Id)
                </div>
                <div class="col-md-6">
                    <span id="emp_Id"> @Html.DisplayFor(model => model.emp_Id, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })</span>
                </div>
            </div>

            <div class="row">
                <div class="col-md-6">
                    @Html.LabelFor(model => model.dafp_AporteLps)
                </div>
                <div class="col-md-6">
                    <span id="dafp_AporteLps">@Html.DisplayFor(model => model.dafp_AporteLps, new { htmlAttributes = new { @class = "text-box single-line", @disabled = "disabled" } })</span>
                    @Html.ValidationMessageFor(model => model.dafp_AporteLps, "", new { @class = "text-danger" })
                </div>
            </div>

            <div class="row">
                <div class="col-md-6">
                    @Html.LabelFor(model => model.afp_Id)
                </div>
                <div class="col-md-6">
                    <span id="afp_Id"> @Html.DisplayFor(model => model.afp_Id, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })</span>
                </div>
            </div>


            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h4>Auditoría</h4>
                </div>
                <div class="ibox-content">
                    <div class="form-horizontal">
                        <table class="table table-striped table-bordered dataTable">
                            <thead>
                                <tr>
                                    <th>Acción</th>
                                    <th>Usuario</th>
                                    <th>Fecha</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td>Creado</td>
                                    @Html.HiddenFor(model => model.dafp_UsuarioCrea)
                                    <td><span id="tbUsuario_usu_NombreUsuario">@Html.DisplayFor(model => model.tbUsuario.usu_NombreUsuario, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</span></td>
                                    <td><span id="dafp_FechaCrea">@Html.DisplayFor(model => model.dafp_FechaCrea, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</span></td>
                                </tr>
                                <tr>
                                    <td>Modificado</td>
                                    @Html.HiddenFor(model => model.dafp_UsuarioModifica)
                                    <td><span id="tbUsuario1_usu_NombreUsuario">@Html.DisplayFor(model => model.tbUsuario1.usu_NombreUsuario, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</span></td>
                                    <td><span id="dafp_FechaModifica">@Html.DisplayFor(model => model.dafp_FechaModifica, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</span></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
                <div class="footer">
                    <button type="button" class="btn btn-default pull-right" data-dismiss="modal">Cerrar</button>
                </div>
            </div>
        </div>
    </div>
}