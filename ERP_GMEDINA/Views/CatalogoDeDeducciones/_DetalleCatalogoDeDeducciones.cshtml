@model ERP_GMEDINA.Models.tbCatalogoDeDeducciones
@using (Html.BeginForm(null, null, FormMethod.Post, new { id = "frmCatalogoDeduccionesDetalle" }))
{
    @Html.AntiForgeryToken()
    <div class="form-horizontal">
        <div class="row">
            <div class="col-md-6">
                @Html.LabelFor(model => model.cde_DescripcionDeduccion, new { htmlAttributes = new { @class = "text-box single-line", @disabled = "disabled" } })
            </div>
            <div class="col-md-6">
                <span id="cde_DescripcionDeduccion">@Html.DisplayFor(model => model.cde_DescripcionDeduccion, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })</span>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                @Html.LabelFor(model => model.tde_IdTipoDedu, new { htmlAttributes = new { @class = "text-box single-line", @disabled = "disabled" } })
            </div>
            <div class="col-md-6">
                <select id="tde_IdTipoDedu" name="tde_IdTipoDedu" class="text-box single-line" disabled></select>
            </div>
        </div>

        <div class="row">
            <div class="col-md-6">
                @Html.LabelFor(model => model.cde_PorcentajeColaborador, new { htmlAttributes = new { @class = "text-box single-line", @disabled = "disabled" } })
            </div>
            <div class="col-md-6">
                <span id="cde_PorcentajeColaborador"> @Html.DisplayFor(model => model.cde_PorcentajeColaborador, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })</span>

            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                @Html.LabelFor(model => model.cde_PorcentajeEmpresa, new { htmlAttributes = new { @class = "text-box single-line", @disabled = "disabled" } })
            </div>
            <div class="col-md-6">
                <span id="cde_PorcentajeEmpresa">@Html.DisplayFor(model => model.cde_PorcentajeEmpresa, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })</span>

            </div>
         </div>
        </div>
        <div class="ibox float-e-margins">
            <div class="ibox-title">
                <h4>Auditoría</h4>
            </div>
            <div class="ibox-content">
                <div class="form-horizontal">
                    <table class="table table-striped table-bordered dataTable">
                        <thead>
                            <tr>
                                <th>Acción</th>
                                <th>Usuario</th>
                                <th>Fecha</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>Creado</td>
                                @Html.HiddenFor(model => model.cde_UsuarioCrea, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })
                                <td><span id="tbUsuario_usu_NombreUsuario">@Html.DisplayFor(model => model.tbUsuario.usu_NombreUsuario, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</span></td>
                                <td><span id="cde_FechaCrea">@Html.DisplayFor(model => model.cde_FechaCrea, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</span></td>
                            </tr>
                            <tr>
                                <td>Modificado</td>
                                @Html.HiddenFor(model => model.cde_UsuarioModifica, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })
                                <td><span id="tbUsuario1_usu_NombreUsuario">@Html.EditorFor(model => model.tbUsuario1.usu_NombreUsuario, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</span></td>
                                <td><span id="cde_FechaModifica">@Html.EditorFor(model => model.cde_FechaModifica, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</span></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <div class="footer">
                    <button type="button" class="btn btn-white pull-right" data-dismiss="modal">Cerrar</button>
                </div>
            </div>
        </div>
        }
