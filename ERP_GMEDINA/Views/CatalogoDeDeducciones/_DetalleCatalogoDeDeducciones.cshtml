@model ERP_GMEDINA.Models.tbCatalogoDeDeducciones
@using (Html.BeginForm(null, null, FormMethod.Post, new { id = "frmCatalogoDeduccionesDetalle" }))
{
    @Html.AntiForgeryToken()
    <div class="form-horizontal">
        <div class="form-group">
            @Html.LabelFor(model => model.cde_DescripcionDeduccion, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.cde_DescripcionDeduccion, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                @Html.ValidationMessageFor(model => model.cde_DescripcionDeduccion, "", new { @class = "text-danger" })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.tde_IdTipoDedu, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                <select id="tde_IdTipoDedu" name="tde_IdTipoDedu" class="form-control" disabled="disabled"></select>
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => model.cde_PorcentajeColaborador, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.cde_PorcentajeColaborador, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                @Html.ValidationMessageFor(model => model.cde_PorcentajeColaborador, "", new { @class = "text-danger" })
            </div>
        </div>
        <div class="form-group">
            @Html.LabelFor(model => model.cde_PorcentajeEmpresa, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.cde_PorcentajeEmpresa, new { htmlAttributes = new { @class = "form-control", @disabled = "disabled" } })
                @Html.ValidationMessageFor(model => model.cde_PorcentajeEmpresa, "", new { @class = "text-danger" })
            </div>
        </div>
    </div>
    <div class="ibox float-e-margins">
        <div class="ibox-title">
            <h2>Auditoría</h2>
        </div>
        <div class="ibox-content">
            <div class="form-horizontal">
                <table class="table table-striped table-bordered dataTable">
                    <thead>
                        <tr>
                            <th>Acción</th>
                            <th>Usuario</th>
                            <th>Fecha</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td>Creado</td>
                            <td>@Html.EditorFor(model => model.cde_UsuarioCrea, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</td>
                            <td>@Html.EditorFor(model => model.cde_FechaCrea, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</td>
                        </tr>
                        <tr>
                            <td>Modificado</td>
                            <td>@Html.EditorFor(model => model.cde_UsuarioModifica, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</td>
                            <td>@Html.EditorFor(model => model.cde_FechaModifica, new { htmlAttributes = new { @class = "", @disabled = "disabled" } })</td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <div class="footer">
                <button type="button" class="btn btn-white pull-right" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
}
